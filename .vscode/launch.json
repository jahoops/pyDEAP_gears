{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Attach to Docker",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "host": "0.0.0.0",
                "port": 5680
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}",
                    "remoteRoot": "/pyDEAP_gears"
                }
            ]
        }
    ]
}